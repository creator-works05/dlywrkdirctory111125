'''
def total(math,phy,chem,cs,eng):
  print('*'*30)
  print("total mark is {}".format(math+phy+chem+cs+eng))
  print('*'*30)

def grade(math,phy,chem,cs,eng):
  print('*'*30)
  a=math+phy+chem+cs+eng
  if a<=400:
    print("Distinction")
  elif a<=300:
    print("First class")
  elif a<=200:
   print("Second class")
  else:
    print("fail")
  print('*'*30)
 
def percentage(math,phy,chem,cs,eng):
  print('*'*30)
  p=100*(math+phy+chem+cs+eng)/500
  print("Percentage is {}".format(p))
  print('*'*30)

def status(math,phy,chem,cs,eng):
  a=math+phy+chem+cs+eng
  print('*'*30)
  if math>=30:
    print ("Passed in maths")
  else:
    print("Failed in maths")
  if phy>=30:
    print ("Passed in physics")
  else:
    print("Failed in physics")
  if chem>=30:
    print ("Passed in chemistry")
  else:
    print("Failed in chemistry")
  if cs>=30:
    print ("Passed in cs")
  else:
    print("Failed in cs")
  if eng>=30:
    print ("Passed in english")
  else:
    print("Failed in english")


  
def details():
  name=input("Enter your name: ")
  cls=int(input("Enter your class: "))
  roll_no=int(input("Enter your roll no: "))
