  while op<1 or op>5:
    print("ERROR.....TRY AGAIN")
    op=int(input("Press 1 for total\nPress 2 for grade\nPress 3 for percentage\nPress 4 for status\nPress 5 to exit\n"))
  if op==1:
    total(math,phy,chem,cs,eng)
  elif op==2:
    grade(math,phy,chem,cs,eng)
  elif op==3:
    percentage(math,phy,chem,cs,eng)
  elif op==4:
    status(math,phy,chem,cs,eng)
  else:
    exit
  print('*'*30)
  while op!=5:
   op=int(input("Press 1 for total\nPress 2 for grade\nPress 3 for percentage\nPress 4 for status\nPress 5 to exit\n"))
   if op==1:
    total(math,phy,chem,cs,eng)
   elif op==2:
    grade(math,phy,chem,cs,eng)
   elif op==3:
    percentage(math,phy,chem,cs,eng)
   elif op==4:
    status(math,phy,chem,cs,eng)
   else:
    exit
